{
  "name": "reschemify",
  "displayName": "Reschemify",
  "description": "Rescheme's Tailwind Components",
  "version": "2.1.0",
  "private": true,
  "homepage": "./",
  "author": "Rescheme Team",
  "main": "index.js",
  "scripts": {
    "build:tailwind2": "postcss src/styles/tailwind.setup.css -c tailwind.config.js -o src/styles/tailwind.build.css",
    "build:tailwind": "npx tailwindcss build ./src/styles/tailwind.setup.css -o ./src/styles/tailwind.build.css",
    "prebuild": "npm run build:tailwind",
    "build": "grunt",
    "build-storybook": "build-storybook -s public",
    "prestart": "npm run build:tailwind",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=4096' start-storybook -p 9009 -s public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/core": "^7.12.*",
    "@babel/preset-env": "^7.12.*",
    "@formatjs/intl-pluralrules": "^3.5.*",
    "@formatjs/intl-relativetimeformat": "^7.3.*",
    "@react-google-maps/api": "^1.13.*",
    "bootstrap": "^5.0.0-alpha3",
    "core-js": "^3.7.*",
    "immutability-helper": "^3.1.*",
    "lodash": "^4.17.*",
    "node-sass": "^5.0.*",
    "oss": "github:rescheme/oss",
    "react": "^17.0.*",
    "react-dom": "^17.0.*",
    "react-scripts": "^4.0.*",
    "sass": "^1.29.*",
    "tailwindcss": "^2.0.*"
  },
  "devDependencies": {
    "@lodder/grunt-postcss": "^3.0.*",
    "@storybook/addon-actions": "^6.1.*",
    "@storybook/addon-links": "^6.1.*",
    "@storybook/addon-viewport": "^6.1.*",
    "@storybook/addons": "^6.1.*",
    "@storybook/preset-create-react-app": "^3.1.*",
    "@storybook/react": "^6.1.*",
    "autoprefixer": "^10.0.*",
    "cross-env": "^7.0.*",
    "cssnano": "^4.1.*",
    "grunt": "^1.3.*",
    "grunt-babel": "^8.0.*",
    "grunt-contrib-clean": "^2.0.*",
    "grunt-contrib-concat": "^1.0.*",
    "grunt-contrib-copy": "^1.0.*",
    "grunt-contrib-cssmin": "^3.0.*",
    "grunt-contrib-jshint": "^3.0.*",
    "grunt-contrib-nodeunit": "^2.1.*",
    "grunt-contrib-uglify": "^5.0.*",
    "grunt-contrib-uglify-es": "^3.3.*",
    "grunt-contrib-watch": "^1.1.*",
    "grunt-postcss": "^0.9.*",
    "grunt-sass": "^3.1.*",
    "grunt-string-replace": "^1.3.*",
    "grunt-terser": "^1.0.*",
    "hex-rgb": "^4.2.*",
    "pixrem": "^5.0.*",
    "postcss": "^8.1.*",
    "postcss-import": "^13.0.*",
    "postcss-loader": "^4.0.*",
    "postcss-nested": "^5.0.*",
    "redux-mock-store": "^1.5.*",
    "sass-loader": "^10.1.*",
    "storybook-react-router": "^1.0.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "test": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
